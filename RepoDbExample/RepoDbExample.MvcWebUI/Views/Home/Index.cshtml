@{
    ViewData["Title"] = "Dashboard Home Page";
}
@model RepoDbExample.MvcWebUI.ViewModels.HomeIndexViewModel

    <div class="row">
        <div class="col-sm-3 col-md-3">
            @{
                await Html.RenderPartialAsync("_FinansPartial", Model.Cashboxes);
            }
        </div>
        <div class="col-sm-9 col-md-9">
            @{
                await Html.RenderPartialAsync("_PostListPartial", Model.Posts);
            }
        </div>
    </div>

    <div class="row">
        <div class="col-sm-12 col-md-12">
            @{
                await Html.RenderPartialAsync("_BookPartial", Model.BookList);
            }
        </div>
        </div>